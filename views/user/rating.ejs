<style>
    .total-like-user-main a {
        display: inline-block;
        margin: 0 -17px 0 0;
    }

    .total-like {
        border: 1px solid;
        border-radius: 50px;
        display: inline-block;
        font-weight: 500;
        height: 34px;
        line-height: 33px;
        padding: 0 13px;
        vertical-align: top;
    }

    .progress {
        background: #f2f4f8;
        border-radius: 0;
        height: 30px;
    }

    .rating-list {
        display: flex;
        margin-bottom: 15px;
        width: 100%;
    }

    .rating-list-left {
        height: 16px;
        line-height: 29px;
        width: 10%;
    }

    .rating-list-center {
        width: 80%;
    }

    .rating-list-right {
        line-height: 29px;
        text-align: right;
        width: 10%;
    }

    .star-rating {
        display: flex;
        align-items: center;
    }

    .star-rating i.active {
        color: #ffc107;
    }

    .reviews-members .media .me-3 {
        width: 56px;
        height: 56px;
        object-fit: cover;
    }

    .total-like-user {
        border: 2px solid #fff;
        height: 34px;
        box-shadow: 0 .125rem .25rem rgba(0, 0, 0, .075) !important;
        width: 34px;
    }

    .rounded-pill {
        border-radius: 50rem !important;
    }

    .fa-star{
        color: #ffc107;
    }
</style>
<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
